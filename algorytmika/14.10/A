#include <cstdio>
using namespace std;
int main(){
	int z = 0;
	scanf ("%d", &z);
	int tab[z][3];
	int t[10][3];
	int sum[3];

	for(int i = 0; i<z; i++){
		for(int j = 0; j<10; j++){
			for(int k = 0; k<3; k++)
				scanf ("%d", t[j][k]);
		}
		for(int j =0; j <3; j++)
			sum[j] = 0;
		for(int j = 0; j<3; j++){
			for (int k = 0; k<10; k++)
				 sum[j]+=[k][j];
		}
		for (int j = 0; j<3; j++)
			printf("%d ",(sum[j]+5)/10);
	}
}

