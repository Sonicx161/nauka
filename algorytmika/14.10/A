#include <cstdio>
using namespace std;
int main(){
	int z = 0;
	scanf ("%d", &z);
	int tab[z][3];
	int t[10][3];
	int sum[3];
	int sum2[3];
	
	for(int i = 0; i<z; i++){
		for(int j = 0; j<10; j++)
			for(int k = 0; k<3; k++)
				scanf ("%d", t[j][k]);
		fill_n(sum, elementCount, 0);
		for(int j = 0; j<3; j++)
			for (int k = 0; k<10; k++)
				 sum[j]+=[k][j];
		for (int j = 0; j<3; j++){		
			tab[z][j] = sum[j]/10;
			sum2[j] += tab[z][j];
		}
	}
	for( int i = 0; i<3; i++)
		printf("%d ",sum2[i]/10);
	return 0;
}

